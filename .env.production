# Production Environment Configuration
# Train Plan Wise - RL Agent Production Setup

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# AI Model Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here
AI_MODEL_VERSION=deepseek-v1
AI_TEMPERATURE=0.2
AI_MAX_TOKENS=4000

# Fleet Configuration
TARGET_PUNCTUALITY=99.5
MIN_FLEET_AVAILABILITY=90
MAX_SERVICE_HOURS=16
MIN_MAINTENANCE_INTERVAL=24

# KMRL System Configuration
MAXIMO_API_URL=https://your-maximo-instance.com
MAXIMO_API_KEY=your_maximo_api_key
WEATHER_API_KEY=your_weather_api_key

# Monitoring & Alerts
ALERT_EMAIL=operations@kmrl.co.in
SLACK_WEBHOOK_URL=your_slack_webhook_url

# Production Optimizations
NODE_ENV=production
VITE_BUILD_TARGET=production
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true

# Database Connection Pool
DB_POOL_SIZE=20
DB_TIMEOUT=30000

# Caching Configuration
REDIS_URL=your_redis_url_here
CACHE_TTL=300

# Security
CORS_ORIGIN=https://your-production-domain.com
JWT_SECRET=your_jwt_secret_here